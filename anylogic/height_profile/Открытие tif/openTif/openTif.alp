        System.out.println("–õ–µ–≤—ã–π –Ω–∏–∂–Ω–∏–π [" + (height - 1) + "][0]: " + fullPixelArray[height - 1][0]);
        System.out.println("–ü—Ä–∞–≤—ã–π –Ω–∏–∂–Ω–∏–π [" + (height - 1) + "][" + (width - 1) + "]: " +
                fullPixelArray[height - 1][width - 1]);

