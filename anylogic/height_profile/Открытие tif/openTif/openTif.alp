			<Text><![CDATA[/**
 * SimpleProfileCreator
 */	
public class SimpleProfileCreator implements Serializable {

    /**
     * –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
     */
    public SimpleProfileCreator() {
    }

	@Override
	public String toString() {
		return super.toString();
	}

	/**
	 * –≠—Ç–æ —á–∏—Å–ª–æ –∏—Å–ø–æ–ª—å–∑—É–µ—Ç—Å—è –ø—Ä–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–∏ —Å–æ—Å—Ç–æ—è–Ω–∏—è –º–æ–¥–µ–ª–∏<br>
	 * –ï–≥–æ —Ä–µ–∫–æ–º–µ–Ω–¥—É–µ—Ç—Å—è –∏–∑–º–µ–Ω–∏—Ç—å –≤ —Å–ª—É—á–∞–µ –∏–∑–º–µ–Ω–µ–Ω–∏—è –∫–ª–∞—Å—Å–∞
	 */ 
	private static final long serialVersionUID = 1L;

    /**
     * –°–æ–∑–¥–∞–µ—Ç –ø—Ä–æ—Ñ–∏–ª—å –≤—ã—Å–æ—Ç –º–µ–∂–¥—É –¥–≤—É–º—è —Ç–æ—á–∫–∞–º–∏.
     *
     * –°–ê–ú–´–ô –ü–†–û–°–¢–û–ô –°–ü–û–°–û–ë –°–û–ó–î–ê–ù–ò–Ø –ü–†–û–§–ò–õ–Ø!
     *
     * @param heightData –º–∞—Å—Å–∏–≤ –≤—ã—Å–æ—Ç –∏–∑ TIF —Ñ–∞–π–ª–∞ [—Å—Ç—Ä–æ–∫–∞][—Å—Ç–æ–ª–±–µ—Ü]
     * @param startX –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞ X –Ω–∞—á–∞–ª—å–Ω–æ–π —Ç–æ—á–∫–∏ (–æ—Ç 0 –¥–æ —à–∏—Ä–∏–Ω–∞-1)
     * @param startY –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞ Y –Ω–∞—á–∞–ª—å–Ω–æ–π —Ç–æ—á–∫–∏ (–æ—Ç 0 –¥–æ –≤—ã—Å–æ—Ç–∞-1)
     * @param endX –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞ X –∫–æ–Ω–µ—á–Ω–æ–π —Ç–æ—á–∫–∏ (–æ—Ç 0 –¥–æ —à–∏—Ä–∏–Ω–∞-1)
     * @param endY –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞ Y –∫–æ–Ω–µ—á–Ω–æ–π —Ç–æ—á–∫–∏ (–æ—Ç 0 –¥–æ –≤—ã—Å–æ—Ç–∞-1)
     * @param numPoints —Å–∫–æ–ª—å–∫–æ —Ç–æ—á–µ–∫ —Å–æ–∑–¥–∞—Ç—å –Ω–∞ –æ—Ç—Ä–µ–∑–∫–µ (–º–∏–Ω–∏–º—É–º 2)
     * @return –¥–≤—É–º–µ—Ä–Ω—ã–π –º–∞—Å—Å–∏–≤ [–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ_—Ç–æ—á–µ–∫][3], –≥–¥–µ –∫–∞–∂–¥–∞—è —Å—Ç—Ä–æ–∫–∞ = [X, Y, Height]
     */
    public static double[][] create(int[][] heightData,
                                     double startX, double startY,
                                     double endX, double endY,
                                     int numPoints) {

        // –ü—Ä–æ–≤–µ—Ä–∫–∞ –≤—Ö–æ–¥–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö
        if (heightData == null || heightData.length == 0) {
            System.err.println("–û–®–ò–ë–ö–ê: –ú–∞—Å—Å–∏–≤ –¥–∞–Ω–Ω—ã—Ö –ø—É—Å—Ç–æ–π!");
            return null;
        }

        if (numPoints < 2) {
            System.err.println("–û–®–ò–ë–ö–ê: –ù—É–∂–Ω–æ –º–∏–Ω–∏–º—É–º 2 —Ç–æ—á–∫–∏!");
            return null;
        }

        // –ü–æ–ª—É—á–∞–µ–º —Ä–∞–∑–º–µ—Ä—ã –º–∞—Å—Å–∏–≤–∞
        int dataHeight = heightData.length;      // –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç—Ä–æ–∫
        int dataWidth = heightData[0].length;    // –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç–æ–ª–±—Ü–æ–≤

        // –°–æ–∑–¥–∞–µ–º –º–∞—Å—Å–∏–≤ –¥–ª—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞
        // –ö–∞–∂–¥–∞—è —Å—Ç—Ä–æ–∫–∞ –±—É–¥–µ—Ç —Å–æ–¥–µ—Ä–∂–∞—Ç—å [X, Y, Height]
        double[][] result = new double[numPoints][3];

        System.out.println("=== –°–û–ó–î–ê–ù–ò–ï –ü–†–û–§–ò–õ–Ø ===");
        System.out.println("–û—Ç —Ç–æ—á–∫–∏ (" + startX + ", " + startY + ")");
        System.out.println("–î–æ —Ç–æ—á–∫–∏ (" + endX + ", " + endY + ")");
        System.out.println("–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ç–æ—á–µ–∫: " + numPoints);

        // –ü—Ä–æ—Ö–æ–¥–∏–º –ø–æ –∫–∞–∂–¥–æ–π —Ç–æ—á–∫–µ –ø—Ä–æ—Ñ–∏–ª—è
        for (int i = 0; i < numPoints; i++) {

            // –®–ê–ì 1: –í—ã—á–∏—Å–ª—è–µ–º –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç t –æ—Ç 0.0 –¥–æ 1.0
            // t = 0.0 —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É–µ—Ç –Ω–∞—á–∞–ª—É, t = 1.0 —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É–µ—Ç –∫–æ–Ω—Ü—É
            double t = (double) i / (numPoints - 1);

            // –®–ê–ì 2: –õ–∏–Ω–µ–π–Ω–∞—è –∏–Ω—Ç–µ—Ä–ø–æ–ª—è—Ü–∏—è –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç
            // –§–æ—Ä–º—É–ª–∞: —Ç–µ–∫—É—â–∞—è_–∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞ = –Ω–∞—á–∞–ª–æ + (–∫–æ–Ω–µ—Ü - –Ω–∞—á–∞–ª–æ) * t
            double currentX = startX + (endX - startX) * t;
            double currentY = startY + (endY - startY) * t;

            // –®–ê–ì 3: –ü—Ä–µ–æ–±—Ä–∞–∑—É–µ–º –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã –≤ –∏–Ω–¥–µ–∫—Å—ã –º–∞—Å—Å–∏–≤–∞
            // –û–∫—Ä—É–≥–ª—è–µ–º –¥–æ –±–ª–∏–∂–∞–π—à–µ–≥–æ —Ü–µ–ª–æ–≥–æ —á–∏—Å–ª–∞
            int col = (int) Math.round(currentX);
            int row = (int) Math.round(currentY);

            // –®–ê–ì 4: –ü—Ä–æ–≤–µ—Ä—è–µ–º –≥—Ä–∞–Ω–∏—Ü—ã –º–∞—Å—Å–∏–≤–∞
            // –ï—Å–ª–∏ –≤—ã—à–ª–∏ –∑–∞ –≥—Ä–∞–Ω–∏—Ü—ã - –æ–±—Ä–µ–∑–∞–µ–º –¥–æ –¥–æ–ø—É—Å—Ç–∏–º—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π
            if (col < 0) col = 0;
            if (col >= dataWidth) col = dataWidth - 1;
            if (row < 0) row = 0;
            if (row >= dataHeight) row = dataHeight - 1;

            // –®–ê–ì 5: –ü–æ–ª—É—á–∞–µ–º –≤—ã—Å–æ—Ç—É –∏–∑ –º–∞—Å—Å–∏–≤–∞
            double height = heightData[row][col];

            // –®–ê–ì 6: –°–æ—Ö—Ä–∞–Ω—è–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç –≤ –º–∞—Å—Å–∏–≤
            result[i][0] = currentX;  // –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞ X
            result[i][1] = currentY;  // –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞ Y
            result[i][2] = height;    // –≤—ã—Å–æ—Ç–∞

            // –í—ã–≤–æ–¥–∏–º –ø–µ—Ä–≤—ã–µ 3 —Ç–æ—á–∫–∏ –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏
            if (i < 3) {
                System.out.println("–¢–æ—á–∫–∞ " + (i + 1) + ": X=" +
                        String.format("%.2f", currentX) + ", Y=" +
                        String.format("%.2f", currentY) + ", H=" +
                        String.format("%.2f", height));
            }
        }

        System.out.println("‚úì –ü—Ä–æ—Ñ–∏–ª—å —Å–æ–∑–¥–∞–Ω!");
        return result;
    }

    /**
     * –°–æ—Ö—Ä–∞–Ω—è–µ—Ç –ø—Ä–æ—Ñ–∏–ª—å –≤ CSV —Ñ–∞–π–ª.
     *
     * –§–æ—Ä–º–∞—Ç —Ñ–∞–π–ª–∞:
     * X;Y;Height
     * 10.50;20.30;1523.45
     * 11.20;21.10;1534.78
     * ...
     *
     * @param profile –º–∞—Å—Å–∏–≤ —Ç–æ—á–µ–∫ [–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ][3], –≥–¥–µ –∫–∞–∂–¥–∞—è —Å—Ç—Ä–æ–∫–∞ = [X, Y, Height]
     * @param filename –∏–º—è —Ñ–∞–π–ª–∞ –¥–ª—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è (–Ω–∞–ø—Ä–∏–º–µ—Ä, "profile.csv")
     * @return true –µ—Å–ª–∏ —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–æ, false –µ—Å–ª–∏ –æ—à–∏–±–∫–∞
     */
    public static boolean saveCSV(double[][] profile, String filename) {

        // –ü—Ä–æ–≤–µ—Ä–∫–∞ –≤—Ö–æ–¥–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö
        if (profile == null || profile.length == 0) {
            System.err.println("–û–®–ò–ë–ö–ê: –ù–µ—Ç –¥–∞–Ω–Ω—ã—Ö –¥–ª—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è!");
            return false;
        }

        try {
            // –û—Ç–∫—Ä—ã–≤–∞–µ–º —Ñ–∞–π–ª –¥–ª—è –∑–∞–ø–∏—Å–∏
            java.io.PrintWriter writer = new java.io.PrintWriter(filename);

            // –ó–∞–ø–∏—Å—ã–≤–∞–µ–º –∑–∞–≥–æ–ª–æ–≤–æ–∫
            writer.println("X;Y;Height");

            // –ó–∞–ø–∏—Å—ã–≤–∞–µ–º –∫–∞–∂–¥—É—é —Ç–æ—á–∫—É
            for (int i = 0; i < profile.length; i++) {
                double x = profile[i][0];
                double y = profile[i][1];
                double height = profile[i][2];

                // –ó–∞–ø–∏—Å—ã–≤–∞–µ–º —Å—Ç—Ä–æ–∫—É –≤ —Ñ–æ—Ä–º–∞—Ç–µ: X;Y;Height
                writer.printf("%.3f;%.3f;%.3f%n", x, y, height);
            }

            // –ó–∞–∫—Ä—ã–≤–∞–µ–º —Ñ–∞–π–ª
            writer.close();

            System.out.println("‚úì –§–∞–π–ª —Å–æ—Ö—Ä–∞–Ω–µ–Ω: " + filename);
            System.out.println("‚úì –ó–∞–ø–∏—Å–∞–Ω–æ —Ç–æ—á–µ–∫: " + profile.length);
            return true;

        } catch (Exception e) {
            System.err.println("–û–®–ò–ë–ö–ê –ø—Ä–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–∏ —Ñ–∞–π–ª–∞: " + e.getMessage());
            e.printStackTrace();
            return false;
        }
    }

    /**
     * –í—ã–≤–æ–¥–∏—Ç –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –ø—Ä–æ—Ñ–∏–ª–µ –≤ –∫–æ–Ω—Å–æ–ª—å.
     *
     * @param profile –º–∞—Å—Å–∏–≤ —Ç–æ—á–µ–∫ –ø—Ä–æ—Ñ–∏–ª—è
     */
    public static void printInfo(double[][] profile) {

        if (profile == null || profile.length == 0) {
            System.out.println("–ù–µ—Ç –¥–∞–Ω–Ω—ã—Ö –¥–ª—è –≤—ã–≤–æ–¥–∞");
            return;
        }

        System.out.println("\n=== –ò–ù–§–û–†–ú–ê–¶–ò–Ø –û –ü–†–û–§–ò–õ–ï ===");
        System.out.println("–í—Å–µ–≥–æ —Ç–æ—á–µ–∫: " + profile.length);

        // –ù–∞—Ö–æ–¥–∏–º –º–∏–Ω–∏–º—É–º –∏ –º–∞–∫—Å–∏–º—É–º –≤—ã—Å–æ—Ç—ã
        double minHeight = Double.MAX_VALUE;
        double maxHeight = Double.MIN_VALUE;
        double sumHeight = 0;

        for (int i = 0; i < profile.length; i++) {
            double h = profile[i][2];
            if (h < minHeight) minHeight = h;
            if (h > maxHeight) maxHeight = h;
            sumHeight += h;
        }

        double avgHeight = sumHeight / profile.length;

        System.out.println("–ú–∏–Ω–∏–º–∞–ª—å–Ω–∞—è –≤—ã—Å–æ—Ç–∞: " + String.format("%.2f", minHeight));
        System.out.println("–ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è –≤—ã—Å–æ—Ç–∞: " + String.format("%.2f", maxHeight));
        System.out.println("–°—Ä–µ–¥–Ω—è—è –≤—ã—Å–æ—Ç–∞: " + String.format("%.2f", avgHeight));
        System.out.println("–ü–µ—Ä–µ–ø–∞–¥ –≤—ã—Å–æ—Ç: " + String.format("%.2f", maxHeight - minHeight));

        // –í—ã–≤–æ–¥–∏–º –ø–µ—Ä–≤—ã–µ 5 —Ç–æ—á–µ–∫
        System.out.println("\n–ü–µ—Ä–≤—ã–µ 5 —Ç–æ—á–µ–∫:");
        System.out.printf("%-5s %-10s %-10s %-10s%n", "‚Ññ", "X", "Y", "Height");
        System.out.println("----------------------------------------");

        int limit = Math.min(5, profile.length);
        for (int i = 0; i < limit; i++) {
            System.out.printf("%-5d %-10.2f %-10.2f %-10.2f%n",
                    (i + 1), profile[i][0], profile[i][1], profile[i][2]);
        }
    }

    /**
     * –î–µ–º–æ–Ω—Å—Ç—Ä–∞—Ü–∏—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –∫–ª–∞—Å—Å–∞.
     */
    public static void demo() {
        System.out.println("========================================");
        System.out.println("–î–ï–ú–û: SimpleProfileCreator");
        System.out.println("========================================\n");

        // –°–æ–∑–¥–∞–µ–º —Ç–µ—Å—Ç–æ–≤—ã–π –º–∞—Å—Å–∏–≤ –≤—ã—Å–æ—Ç 100x100
        System.out.println("–°–æ–∑–¥–∞–Ω–∏–µ —Ç–µ—Å—Ç–æ–≤—ã—Ö –¥–∞–Ω–Ω—ã—Ö 100x100...");
        int[][] testData = new int[100][100];

        for (int row = 0; row < 100; row++) {
            for (int col = 0; col < 100; col++) {
                // –ü—Ä–æ—Å—Ç–∞—è —Ñ–æ—Ä–º—É–ª–∞ –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –≤—ã—Å–æ—Ç (–≥—Ä–∞–¥–∏–µ–Ω—Ç)
                testData[row][col] = row * 10 + col * 5;
            }
        }

        System.out.println("‚úì –¢–µ—Å—Ç–æ–≤—ã–µ –¥–∞–Ω–Ω—ã–µ —Å–æ–∑–¥–∞–Ω—ã\n");

        // –ì–õ–ê–í–ù–ê–Ø –û–ü–ï–†–ê–¶–ò–Ø: –°–æ–∑–¥–∞–Ω–∏–µ –ø—Ä–æ—Ñ–∏–ª—è
        // –û—Ç —Ç–æ—á–∫–∏ (10, 20) –¥–æ —Ç–æ—á–∫–∏ (90, 80) —Å 50 —Ç–æ—á–∫–∞–º–∏

        double[][] profile = create(
                testData,   // –º–∞—Å—Å–∏–≤ –¥–∞–Ω–Ω—ã—Ö
                10, 20,     // –Ω–∞—á–∞–ª—å–Ω–∞—è —Ç–æ—á–∫–∞ (X=10, Y=20)
                90, 80,     // –∫–æ–Ω–µ—á–Ω–∞—è —Ç–æ—á–∫–∞ (X=90, Y=80)
                50          // –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ç–æ—á–µ–∫
        );

        // –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç
        if (profile != null) {
            // –í—ã–≤–æ–¥–∏–º –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é
            printInfo(profile);

            // –°–æ—Ö—Ä–∞–Ω—è–µ–º –≤ CSV
            saveCSV(profile, "simple_profile_demo.csv");

            System.out.println("\n‚úì –ì–æ—Ç–æ–≤–æ! –ü—Ä–æ—Ñ–∏–ª—å —Å–æ–∑–¥–∞–Ω –∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω.");
        } else {
            System.out.println("‚úó –û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ–∑–¥–∞–Ω–∏–∏ –ø—Ä–æ—Ñ–∏–ª—è");
        }
    }

    /**
     * –ì–ª–∞–≤–Ω—ã–π –º–µ—Ç–æ–¥ –¥–ª—è —Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏—è
     */
    public static void main(String[] args) {
        demo();
    }

