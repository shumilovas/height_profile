			<Name><![CDATA[FullTIFFReader]]></Name>
			<Text><![CDATA[/**
 * FullTIFFReader
 */	

import java.awt.image.BufferedImage;
import java.awt.image.Raster;
import java.io.File;
import java.io.PrintStream;
import java.io.PrintWriter;
import java.util.Iterator;
import javax.imageio.ImageIO;
import javax.imageio.ImageReader;
import javax.imageio.stream.ImageInputStream;
public class FullTIFFReader implements Serializable {
	private static int[][] fullPixelArray;
    private static int width;
    private static int width_segment;
    private static int height_segment;
    private static int height;
    public static int x_begin;
    public static int y_begin;
    public static int x_end;
    public static int y_end;
    //private static int x_begin;
    public static int number_segment;




    /**
     * –ö–æ–Ω—Å—Ç—Ä—É–∫—Ç–æ—Ä –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
     */
    public FullTIFFReader() {
    }

	@Override
	public String toString() {
		return super.toString();
	}

	/**
	 * –≠—Ç–æ —á–∏—Å–ª–æ –∏—Å–ø–æ–ª—å–∑—É–µ—Ç—Å—è –ø—Ä–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–∏ —Å–æ—Å—Ç–æ—è–Ω–∏—è –º–æ–¥–µ–ª–∏<br>
	 * –ï–≥–æ —Ä–µ–∫–æ–º–µ–Ω–¥—É–µ—Ç—Å—è –∏–∑–º–µ–Ω–∏—Ç—å –≤ —Å–ª—É—á–∞–µ –∏–∑–º–µ–Ω–µ–Ω–∏—è –∫–ª–∞—Å—Å–∞
	 */ 
	private static final long serialVersionUID = 1L;
	
	public static void main1() {
        try {
            String filePath = "out.tif";
            File tiffFile = new File(filePath);
            if (!tiffFile.exists()) {
                System.out.println("–§–∞–π–ª –Ω–µ –Ω–∞–π–¥–µ–Ω: " + filePath);
                return;
            }

            boolean success = readTIFFToArray(tiffFile);
            if (success) {
                System.out.println("‚úì TIFF —Ñ–∞–π–ª —É—Å–ø–µ—à–Ω–æ –ø—Ä–æ—á–∏—Ç–∞–Ω!");
                System.out.println("‚úì –†–∞–∑–º–µ—Ä—ã: " + width + "x" + height);
                System.out.println("‚úì –í—Å–µ–≥–æ —ç–ª–µ–º–µ–Ω—Ç–æ–≤: " + width * height);
                showArrayInfo();
                saveArrayToCSV("height_profile.csv");
                saveNotFullArrayToCSV("not_full_height_profile.csv");
                processFullArray();
                accessSpecificElements();
            }
        } catch (Exception e) {
            System.err.println("–û—à–∏–±–∫–∞:");
            e.printStackTrace();
        }
        
        

    }
	
	private static boolean readTIFFToArray(File tiffFile) throws Exception {
        Iterator<ImageReader> readers = ImageIO.getImageReadersByFormatName("TIFF");
        if (!readers.hasNext()) {
            System.out.println("–û—à–∏–±–∫–∞: TIFF —Ä–∏–¥–µ—Ä –Ω–µ –Ω–∞–π–¥–µ–Ω.");
            return false;
        } else {
            ImageReader reader = (ImageReader)readers.next();

            boolean var16;
            try {
                ImageInputStream stream = ImageIO.createImageInputStream(tiffFile);

                try {
                    reader.setInput(stream);
                    width = reader.getWidth(0);
                    height = reader.getHeight(0);
                    System.out.println("–ß—Ç–µ–Ω–∏–µ —Ñ–∞–π–ª–∞...");
                    System.out.println("–†–∞–∑–º–µ—Ä—ã: " + width + "x" + height);
                    BufferedImage image = reader.read(0);
                    Raster raster = image.getRaster();
                    fullPixelArray = new int[height][width];
                    System.out.println("–ß—Ç–µ–Ω–∏–µ " + width * height + " –ø–∏–∫—Å–µ–ª–µ–π...");
                    int[] pixel = new int[raster.getNumBands()];

                    for(int y = 0; y < height; ++y) {
                        for(int x = 0; x < width; ++x) {
                            raster.getPixel(x, y, pixel);
